{
  "name": "docs",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "renoun next build && pnpm generate-pagefind",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "renoun next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --check --write . --ignore-path ../../.gitignore",
    "gen:graphql": "tsx src/generators/graphql/graphql-to-mdx.ts",
    "generate-pagefind": "pagefind --site out --output-path out/pagefind",
    "info": "next info",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "list:tests": "playwright test --list --reporter \"./tests/doc-reporter.ts\"",
    "preview": "tsx src/localserver.ts",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@acme/prettier-config",
  "dependencies": {
    "@acme/ui": "workspace:*",
    "@getcanary/web": "1.0.0",
    "@giscus/react": "3.0.0",
    "@hapi/bourne": "3.0.0",
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next/mdx": "14.2.14",
    "@playwright/test": "1.47.2",
    "@renoun/mdx": "1.1.0",
    "execa": "9.4.0",
    "lucide-react": "0.447.0",
    "multimatch": "7.0.0",
    "next": "14.2.14",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "renoun": "5.2.0",
    "restyle": "2.4.0",
    "tm-themes": "1.8.7",
    "ts-dot-prop": "2.1.4",
    "zod": "3.23.8",
    "zustand": "5.0.0-rc.2"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@graphql-markdown/core": "1.12.0",
    "@graphql-markdown/graphql": "1.1.4",
    "@graphql-markdown/printer-legacy": "1.9.0",
    "@graphql-markdown/types": "1.4.0",
    "@graphql-markdown/utils": "1.7.0",
    "@graphql-tools/graphql-file-loader": "8.0.1",
    "@types/mdx": "2.0.13",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/serve-handler": "6.1.4",
    "eslint": "9.12.0",
    "pagefind": "1.1.1",
    "playwright": "1.47.2",
    "prettier": "3.3.3",
    "serve-handler": "6.1.5",
    "tailwindcss": "3.4.13",
    "tsx": "4.19.1",
    "typescript": "5.6.2"
  }
}
