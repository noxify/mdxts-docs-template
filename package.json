{
  "name": "mdxts-docs-template",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "mdxts next build",
    "deps:check": "pnpm outdated -r",
    "deps:update": "pnpm update -i -L -r",
    "dev": "concurrently \"mdxts watch\" \"next\"",
    "format": "prettier --check . --ignore-path .gitignore",
    "format:fix": "prettier --check --write . --ignore-path .gitignore",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "@radix-ui/react-slot": "1.1.0",
    "class-variance-authority": "0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "0.427.0",
    "mdxts": "1.6.0",
    "next": "14.2.5",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^2.5.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@eslint/compat": "1.1.1",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@next/eslint-plugin-next": "14.2.5",
    "@types/node": "22.2.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "concurrently": "8.2.2",
    "eslint": "9.9.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-package-json": "0.15.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "rc",
    "eslint-plugin-turbo": "2.0.12",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "tailwindcss": "3.4.9",
    "typescript": "5.5.4",
    "typescript-eslint": "8.0.1"
  }
}
