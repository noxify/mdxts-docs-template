{
  "name": "omnidoc-docs-template",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "omnidoc next build",
    "deps:check": "pnpm outdated -r",
    "deps:update": "pnpm update -i -L -r",
    "dev": "omnidoc next dev",
    "format": "prettier --check . --ignore-path .gitignore",
    "format:fix": "prettier --check --write . --ignore-path .gitignore",
    "info": "next info",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "tsx src/localserver.ts",
    "typecheck": "tsc --noEmit",
    "ui": "shadcn"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "@giscus/react": "3.0.0",
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next/mdx": "14.2.8",
    "@omnidoc/mdx-plugins": "0.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@types/mdx": "2.0.13",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "0.439.0",
    "multimatch": "7.0.0",
    "next": "14.2.8",
    "next-themes": "0.3.0",
    "omnidoc": "3.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "restyle": "2.2.1",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "tm-themes": "1.8.1",
    "zod": "3.23.8",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@eslint/compat": "1.1.1",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@next/eslint-plugin-next": "14.2.8",
    "@tailwindcss/typography": "0.5.15",
    "@types/mdx": "2.0.13",
    "@types/node": "22.5.4",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/serve-handler": "6.1.4",
    "concurrently": "8.2.2",
    "eslint": "9.10.0",
    "eslint-config-next": "14.2.8",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-package-json": "0.15.2",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "rc",
    "postcss": "8.4.45",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "serve-handler": "6.1.5",
    "shadcn": "2.0.4",
    "tailwindcss": "3.4.10",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.4.0"
  },
  "pnpm": {
    "overrides": {
      "minimist@<0.2.1": ">=0.2.1",
